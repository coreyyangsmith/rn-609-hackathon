{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(auth)/register.jsx",
    "app/(auth)/index.jsx",
    "app/(app)/(patient)/questionnaire/_layout.jsx",
    "app/(app)/(patient)/questionnaire/index.jsx",
    "app/(app)/index.jsx",
    "app/_layout.jsx"
  ]
}